    "react-dom": "^16.1.1",
    "rimraf": "^2.5.0",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "prop-types": "^15.6.0"
  },
  "bugs": {
    "url": "https://github.com/pixd/react-dom-portal/issues"

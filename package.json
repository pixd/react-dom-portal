{
  "name": "react-dom-portal",
  "version": "0.1.2",
  "description": "React component for transportation React elements",
  "main": "build/portal",
  "files": [
    "*.md",
    "LICENSE",
    "lib",
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixd/react-dom-portal.git"
  },
  "author": "Yakov Dalinchuk <yakov.dalinchuk@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rimraf build && mkdir build && babel ./lib/portal.js --out-file ./build/portal.js",
    "examples": "rimraf examples/static && mkdir examples/static && cross-env NODE_ENV=production webpack --config webpack.prod.config.js",
    "lint": "mocha test/eslint.spec.js",
    "prepublish": "cross-env NODE_ENV=production npm run build",
    "start": "node dev-server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "tags": [
    "react",
    "react-component"
